# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenRouter AI API
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Google Cloud Vision API
GOOGLE_APPLICATION_CREDENTIALS=./google-vision-key.json
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id

# Replicate API (Fallback Vision Service)
REPLICATE_API_TOKEN=your_replicate_api_token

# Weather API
OPENWEATHER_API_KEY=your_openweather_api_key
OPENWEATHER_BASE_URL=https://api.openweathermap.org/data/2.5

# Redis Configuration (for caching and session management)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0
REDIS_PREFIX=lifelens:

# Server Configuration
PORT=3000
NODE_ENV=development
API_BASE_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# File Upload Configuration
UPLOAD_MAX_SIZE=10485760
UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/webp

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:8081,exp://localhost:19000

# Logging
LOG_LEVEL=info
LOG_FILE=error.log

# AI Service Configuration
AI_CACHE_TTL=300
AI_REQUEST_TIMEOUT=30000
AI_MAX_RETRIES=3

# Sentiment Service
SENTIMENT_SERVICE_URL=http://localhost:8000
SENTIMENT_CONFIDENCE_THRESHOLD=0.7

# Feature Flags
ENABLE_AUTO_DECISIONS=true
ENABLE_PHOTO_ANALYSIS=true
ENABLE_MOOD_TRACKING=true
ENABLE_WEATHER_INTEGRATION=true
ENABLE_NUDGE_ENGINE=true

# Analytics and Monitoring
SENTRY_DSN=your_sentry_dsn
ANALYTICS_ENABLED=false

# Database Configuration (if using external database)
DATABASE_URL=postgresql://user:password@localhost:5432/lifelens

# Security Configuration
BCRYPT_ROUNDS=12
SESSION_SECRET=your_session_secret
CSRF_SECRET=your_csrf_secret

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_app_password
FROM_EMAIL=noreply@lifelens.com